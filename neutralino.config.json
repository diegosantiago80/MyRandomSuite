  {
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
  "applicationId": "com.myrandomsuite.pro",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/resources/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "debug.log"
  ],
  "modes": {
    "window": {
      "title": "MyRandomSuite PRO",
      "width": 1100,
      "height": 750,
      "minWidth": 800,
      "minHeight": 600,
      "center": true,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "resources/logo.png",
      "enableInspector": true,
      "borderless": false,
      "maximize": true,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": true
    }
  },
  "cli": {
    "binaryName": "MyRandomSuite",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/resources/js/neutralino.js",
    "binaryVersion": "5.5.0",
    "clientVersion": "5.5.0"
  }
}